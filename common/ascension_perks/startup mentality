ap_startup_mentality = {
    potential = {
        NOR = {
            is_hive_mind = yes
            is_machine_empire = yes
        }
        OR = {
            has_ethic = ethic_materialist
            has_ethic = ethic_fanatic_materialist
        }
    }
    modifier = {
        pop_growth_speed = 0.1
    }
    ai_weight = {
		factor = 1
		modifier = {
			factor = 7
			has_ethic = ethic_materialist
		}
		modifier = {
			factor = 13
			has_ethic = ethic_fanatic_materialist
		}
	}
}